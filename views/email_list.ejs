<%- include("./partials/headers.ejs"); %>
  <div class="wrapper">
    <%- include("./partials/topbar.ejs"); %>
    <%- include("./partials/sideMenu.ejs"); %>
    <!-- Main content div -->
    <div class='content-wrapper'>
        <div class='container m-auto py-3'>
            <div class="card">
                <div class="card-header border-transparent">
                    <h3 class="card-title">EMAILS</h3>
                    <div class="card-tools">
                        <a href="/email"><button type="button" class="btn btn-tool"><i class="fas fa-plus"></i></button></a>
                    </div>
                </div>
                <div class="card-body p-0">
                    <div class="table-responsive">
                        <table class="table m-0">
                            <thead><tr><th>ID</th><th>FULL NAME</th><th>TYPE</th><th>OWNER</th><th>ADDRESS</th><th>NUMBER</th><th>PRIMARY EMAIL</th><th>PASSWORD</th><th>ACTION</th></tr></thead>
                            <tbody><%- locals.emails %></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- middle footer -->
    <%- include("./partials/middle_footer.ejs"); %>
  </div>
<%- include("./partials/footer.ejs"); %>